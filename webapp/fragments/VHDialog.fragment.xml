<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<TableSelectDialog noDataText="No RuleName Found" title="Select RuleName" search=".handleSearch" confirm="handleValueHelpClose"
		cancel="handleValueHelpClose" showClearButton="true" items="{RuleNameList>/}" multiSelect="true" contentWidth="8rem">
		<columns>
			<Column>
				<header>
					<Text text="Region"/>
				</header>
			</Column>
		</columns>
		<ColumnListItem selected="{RuleNameList>selected}">
			<cells>
				<ObjectIdentifier title="{RuleNameList>desc}"/>
			</cells>
		</ColumnListItem>
	</TableSelectDialog>
</core:FragmentDefinition>