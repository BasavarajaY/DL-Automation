<core:FragmentDefinition xmlns="sap.m" xmlns:core="sap.ui.core">
	<QuickView
	width="160px"
		id="quickView"
		pages="{
			path: '/pages',
			templateShareable: true
			}">
		<QuickViewPage
			pageId="{pageId}"
			header="{header}"
			groups="{
				path: 'groups',
				templateShareable: true
			}">
			<QuickViewGroup elements="{ path: 'elements', templateShareable: true}">
				<QuickViewGroupElement label="{label}" value="{value}"/>
				<!--<customData>-->
				<!--	<core:CustomData key="{value}" value="{value}"/>-->
				<!--</customData>-->
			</QuickViewGroup>
		</QuickViewPage>
	</QuickView>
</core:FragmentDefinition>