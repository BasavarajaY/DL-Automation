<mvc:View xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" controllerName="com.extentia.dlrulecreate.controller.DLList"
	xmlns:html="http://www.w3.org/1999/xhtml" xmlns:l="sap.ui.layout" xmlns:fb="sap.ui.comp.filterbar" displayBlock="true"
	xmlns:table="sap.ui.table">
	<fb:FilterBar search="onSearch" id="FilterBar" reset="onReset" showRestoreOnFB="false" showRestoreButton="false" filterBarExpanded="true"
		filterContainerWidth="15rem">
		<fb:filterItems>
			<fb:FilterItem id="ruleName" name="RuleName" label="Rule Name" labelTooltip="RuleName" mandatory="false" visible="true">
				<fb:control>
					<MultiInput id="multiInput" suggestionItems="{RuleNameList>/}" valueHelpRequest="handleValueHelp">
						<core:Item key="{RuleNameList>key}" text="{RuleNameList>desc}"/>
					</MultiInput>
					<!--<MultiInput id="inputRuleName" valueHelpRequest="RuleNameF4"></MultiInput>-->
				</fb:control>
			</fb:FilterItem>
			<fb:FilterItem id="Region" name="Region" label="Region" labelTooltip="Region" mandatory="false" visible="true">
				<fb:control>
					<MultiInput id="inputRegionF4" valueHelpRequest="RegionF4"></MultiInput>
				</fb:control>
			</fb:FilterItem>
			<fb:FilterItem id="SubRegion" name="SubRegion" label="SubRegion" labelTooltip="SubRegion" mandatory="false" visible="true">
				<fb:control>
					<MultiInput id="inputSubRegionF4" valueHelpRequest="SubRegionF4"></MultiInput>
				</fb:control>
			</fb:FilterItem>
			<fb:FilterItem id="CompanyCode" name="CompanyCode" label="CompanyCode" labelTooltip="CompanyCode" mandatory="false" visible="true">
				<fb:control>
					<MultiInput id="inputCompanyCodeF4" valueHelpRequest="CompanyCodeF4"></MultiInput>
				</fb:control>
			</fb:FilterItem>
			<fb:FilterItem id="BusinessUnit" name="BusinessUnit" label="Business Unit" labelTooltip="Business Unit" mandatory="false" visible="false">
				<fb:control>
					<MultiInput id="inputBusinessUnitF4" valueHelpRequest="BusinessUnitF4"></MultiInput>
				</fb:control>
			</fb:FilterItem>
		</fb:filterItems>
	</fb:FilterBar>
	<Panel xmlns="sap.m" class="sapUiResponsiveContentPadding" backgroundDesign="Solid">
		<content>
			<table:Table id="UIRuleTable" rows="{ListItems>/}" showColumnVisibilityMenu="true" enableGrouping="false" selectionMode="None"
				enableSelectAll="false" noData="No Data" enableColumnFreeze="true" minAutoRowCount="8" visibleRowCount="8"
				visible="{= (${device>/system/phone} || ${device>/system/tablet})? false : true}" visibleRowCountMode="Fixed">
				<table:toolbar>
					<Toolbar id="UIListTableToolBar">
						<Title id="ListTableTitle"
							text="{i18n>List.Table.Title} {= (!${LocalViewSetting>/ListItemsCount} > 0) ? '' : '('+${LocalViewSetting>/ListItemsCount}+')'}" level="H2"/>
						<ToolbarSpacer/>
						<Button id="RuleCreate" icon="sap-icon://create" press="handleRuleCreate" text="{i18n>List.Create.Rule}" tooltip="{i18n>List.Create.Rule}">
							<customData>
								<core:CustomData key="multi" value="true"/>
								<core:CustomData key="remember" value="true"/>
							</customData>
						</Button>
						<Button id="ListTableExportButton" icon="sap-icon://download" tooltip="{i18n>List.Table.ExportToExcelTooltip}" press="exportToExcel"
							visible="{= ${device>/system/phone}? false : true}"/>
					</Toolbar>
				</table:toolbar>
				<table:Column sortProperty="RuleName" filterProperty="RuleName" autoResizable="true" tooltip="RuleName">
					<Text text="RuleName"/>
					<table:template>
						<Link text="{= ${ListItems>RuleID} !== '' ? ${ListItems>RuleName} +' ('+ ${ListItems>RuleID}+')' : ${ListItems>RuleName} }"
							press="onRuleIdPress"></Link>
					</table:template>
				</table:Column>
				<table:Column sortProperty="DLName" width="9%" filterProperty="DLName" autoResizable="true" tooltip="DLName">
					<Text text="DL Name"/>
					<table:template>
						<Text text="{ListItems>DLName}" tooltip="DLName"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="DLDescription" width="7%" filterProperty="DLDescription" autoResizable="true" tooltip="DLName" visible="false">
					<Text text="DL Description"/>
					<table:template>
						<Text text="{ListItems>DLDescription}" tooltip="DLDescription"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="Region" width="9%" filterProperty="Region" autoResizable="true" tooltip="Region">
					<Text text="Region"/>
					<table:template>
						<Text text="{ListItems>Region}" tooltip="Region"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="SubRegion" width="8%" filterProperty="SubRegion" autoResizable="true" tooltip="SubRegion">
					<Text text="SubRegion"/>
					<table:template>
						<Button id="SRQuickView" icon="sap-icon://background" text=" " press="handleSRQuickViewPress" tooltip="SubRegion" textDirection="Inherit"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="PrimaryHub" filterProperty="PrimaryHub" autoResizable="true" tooltip="PrimaryHub">
					<Text text="PrimaryHub"/>
					<table:template>
						<Text text="{ListItems>PrimaryHub}" tooltip="PrimaryHub"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="PrimaryHubDL" filterProperty="PrimaryHubDL" autoResizable="true" tooltip="PrimaryHubDL">
					<Text text="PrimaryHubDL"/>
					<table:template>
						<Text text="{ListItems>PrimaryHubDL}" tooltip="PrimaryHubDL"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="SecondaryHub" filterProperty="SecondaryHub" autoResizable="true" tooltip="SecondaryHub">
					<Text text="SecondaryHub"/>
					<table:template>
						<Text text="{ListItems>SecondaryHub}" tooltip="SecondaryHub"/>
					</table:template>
				</table:Column>
				<table:Column sortProperty="SecondaryHubDL" filterProperty="SecondaryHubDL" autoResizable="true" tooltip="SecondaryHubDL">
					<Text text="SecondaryHubDL"/>
					<table:template>
						<Text text="{ListItems>SecondaryHubDL}" tooltip="SecondaryHubDL"/>
					</table:template>
				</table:Column>
